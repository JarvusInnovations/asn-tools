    "http://purl.org/dc/terms/description" : [
      { "value" : "Describe the principles of design in artistic composition<ul><li>balance (e.g., asymmetrical, radial, symmetrical)<\/li><li>contrast (e.g., color, form, line, shape, space, texture, value)<\/li><li>emphasis (e.g., dominance, focal point)<\/li><li>movement (e.g., depth, flow, overlap, placement, repetition, size, time)<\/li><li>pattern (e.g., formal, informal, repetition)<\/li><li>proportion (e.g., exaggeration, scale)<\/li><li>rhythm (e.g., pattern, repetition)<\/li><li>unity (e.g., repetition, order)<\/li><li>harmony (e.g., similarity, wholeness)<\/li><li>variety (e.g., difference, diversity)<\/li><\/ul>", "type" : "literal", "lang" : "en-US" }
    ],

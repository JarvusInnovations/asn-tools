    "http://purl.org/dc/elements/1.1/title" : [
      { "value" : "English Language Arts (ELA) Drama Curriculum Framework", "type" : "literal", "lang" : "en-US" }
    ],
    "http://purl.org/dc/terms/description" : [
      { "value" : "ELA Drama is a one-semester English elective course designed to introduce students to the study of dramatic literature. Through the study of written plays, students will become informed, perceptive, and appreciative audience members of plays from various cultures and time periods. To accomplish this goal, students will gain knowledge of dramatic elements by studying classical to contemporary plays. Students will examine and compare historical influences and contexts, universal themes in dramatic literature, and authorial treatment of tragic heroes from various literary periods. Students will demonstrate understanding of dramatic literature through creative oral and written interpretations and adaptations.", "type" : "literal", "lang" : "en-US" }
    ],
    "http://purl.org/dc/terms/source" : [
      { "value" : "http://www.arkansased.org/public/userfiles/Learning_Services/Curriculum%20and%20Instruction/Frameworks/Eng%20Lang%20Arts/ELA_Drama_CCSS_2013_2014.pdf", "type" : "uri" }
